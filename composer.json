{
	"name": "absolutfussball/php-onefootball-api",
	"type": "library",
	"description": "OneFootball API client",
	"homepage": "https://github.com/Absolut-Fussball/php-onefootball-api",
	"keywords": ["OneFootball", "api"],
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Wiedemann"
		}

	],
	"require": {
		"php": "^7.3",
		"ext-xml": "*",
		"guzzlehttp/psr7": "^1.2",
		"php-http/client-common": "^1.6 || ^2.0",
		"php-http/client-implementation": "^1.0",
		"php-http/discovery": "^1.2",
		"php-http/httplug": "^1.1 || ^2.0",
		"php-http/multipart-stream-builder": "^1.0",
		"symfony/options-resolver": "^2.6 || ^3.0 || ^4.0 || ^5.0"
	},
	"require-dev": {
		"php-http/guzzle6-adapter": "^1.0 || ^2.0",
		"php-http/mock-client": "^1.2",
		"phpunit/phpunit": "^5.7.27 || ^7.5.15"
	},
	"autoload": {
		"psr-4": { "OneFootball\\": "lib/OneFootball/" }
	},
	"autoload-dev": {
		"psr-4": { "OneFootball\\Tests\\": "test/OneFootball/Tests/" }
	}
}
